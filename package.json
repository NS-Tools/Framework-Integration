{
    "name": "ns-tools-integration",
    "version": "1.0.0",
    "devDependencies": {
        "@biomejs/biome": "2.2.7",
        "@hitc/netsuite-types": "^2025.2.4",
        "typescript": "^5.9.3",
        "jest": "29.7.0",
        "@types/jest": "29.5.14",
        "@oracle/suitecloud-unit-testing": "^1.7.0"
    },
    "dependencies": {
    },
    "scripts": {
        "lint": "npx @biomejs/biome lint",
        "lint:fix": "npx @biomejs/biome check --write",
        "test": "jest",
        "build": "./build.sh",
        "thirdparty:setup": "git submodule add --force https://github.com/NS-Tools/ThirdParty.git thirdparty/optional",
        "thirdparty:update": "git submodule update --remote --merge --recursive",
        "update": "git pull origin && npm run update:framework && npm install",
        "update:framework": "git submodule update --remote --recursive"
    }
}
